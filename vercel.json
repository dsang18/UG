{
    "version": 2,
    "builds": [
        {
            "src" : "uttam_pro/wsgi.py",
            "use" : "@vercel/python",
            "config": {"maxLambdaSize" : "15mb", "runtime": "python3.10"}
        },
        {
            "src" : "build_files.sh"
            
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "uttam_pro/wsgi.py"
        }
    ]
}